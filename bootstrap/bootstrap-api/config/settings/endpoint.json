{  
    "lookup_host":  "http://lookup:8080",
    "lookup_group": [
        {
            "endpoint": "/orgs/{name}",
            "method": "POST",
            "backend": "/orgs/{name}"
        },
        {
            "endpoint": "/orgs/{name}/devices/{id}",
            "method": "POST",
            "backend": "/orgs/{name}/devices/{id}"
        },
        {
            "endpoint": "/orgs/{name}/devices/{id}",
            "method": "GET",
            "backend": "/orgs/{name}/devices/{id}"
        },
        {
            "endpoint": "/ping",
            "method": "GET",
            "backend": "/ping",
            "no_auth": "true"
        },
        {
            "endpoint": "/ready",
            "method": "GET",
            "backend": "/ping",
            "no_auth": "true"
        }
    ]
   
}