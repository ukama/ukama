{
  "name": "console-bff",
  "version": "1.0.0",
  "scripts": {
    "seed": "prisma db seed",
    "codegen-metrics": "graphql-codegen",
    "migrate": "prisma migrate dev",
    "gateway": "ts-node --esm ./gateway/index.ts",
    "prettier": "prettier --write --config .prettierrc './**/**'",
    "dev": "concurrently \"yarn planning-tool-dev\" \"yarn gateway\"",
    "planning-tool-dev": "ts-node-dev --no-notify --respawn --transpile-only ./planning-tool/src/index.ts -P tsconfig-dev.json"
  },
  "prisma": {
    "seed": "ts-node ./planning-tool/prisma/seed.ts",
    "schema": "./planning-tool/prisma/schema.prisma"
  },
  "dependencies": {
    "@apollo/gateway": "^2.4.9",
    "@apollo/server": "^4.7.5",
    "@apollo/subgraph": "^2.4.9",
    "@kvs/node-localstorage": "^2.1.3",
    "@prisma/client": "4.16.2",
    "apollo-server": "^3.12.0",
    "apollo-server-core": "^3.12.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "graphql": "latest",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "1.22.2",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^4.0.3",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.2",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@kvs/types": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.4.1",
    "@types/ws": "^8.5.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  }
}
