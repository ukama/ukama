{
  "name": "console-bff",
  "version": "1.0.0",
  "scripts": {
    "seed": "prisma db seed",
    "migrate": "prisma migrate dev",
    "clear-cache": "rm -rf .cache/*",
    "gateway": "ts-node --esm ./gateway/index.ts",
    "prettier": "prettier --write --config .prettierrc './**/**'",
    "node-dev": "yarn clear-cache && ts-node-dev --no-notify --respawn --transpile-only ./node/index.ts",
    "dev": "yarn clear-cache && concurrently \"yarn planning-tool-dev\" \"yarn node-dev\"  \"yarn gateway\"",
    "metrics-dev": "yarn clear-cache && ts-node-dev --no-notify --respawn --transpile-only ./metrics/src/index.ts",
    "planning-tool-dev": "ts-node-dev --no-notify --respawn --transpile-only ./planning-tool/src/index.ts",
    "user-dev": "ts-node-dev --no-notify --respawn --transpile-only ./user/index.ts",
    "org-dev": "ts-node-dev --no-notify --respawn --transpile-only ./org/index.ts"
  },
  "prisma": {
    "seed": "ts-node ./planning-tool/prisma/seed.ts",
    "schema": "./planning-tool/prisma/schema.prisma"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^6.0.1",
    "@apollo/gateway": "^2.4.9",
    "@apollo/server": "^4.7.5",
    "@apollo/subgraph": "^2.4.9",
    "@graphql-tools/schema": "^10.0.0",
    "@kvs/node-localstorage": "^2.1.3",
    "@prisma/client": "4.16.2",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "graphql": "latest",
    "graphql-scalars": "1.22.2",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "stripe": "^12.16.0",
    "type-graphql": "^2.0.0-beta.2",
    "typedi": "^0.10.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@kvs/types": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.4.1",
    "@types/ws": "^8.5.5",
    "concurrently": "^8.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "^4.16.2",
    "string-width": "^6.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
}
