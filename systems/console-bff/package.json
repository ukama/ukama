{
  "name": "console-bff",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn prisma-pre-build && tsc",
    "dev": "yarn all-dev",
    "seed": "prisma db seed",
    "lint": "eslint --ext .ts .",
    "migrate": "prisma migrate dev",
    "clear-cache": "rm -rf .cache/*",
    "start": "yarn build && yarn all-start",
    "prettier": "prettier --write --config .prettierrc './**/**'",
    "metrics-start": "yarn build && yarn clear-cache & node dist/metrics/index.js",
    "prisma-pre-build": "prisma generate --schema=./planning-tool/prisma/schema.prisma && prisma db push --schema=./planning-tool/prisma/schema.prisma",
    "org-dev": "nodemon --watch \"org/**\" --ext \"ts,json,graphql\" --exec \"ts-node org/index.ts\"",
    "user-dev": "nodemon --watch \"user/**\" --ext \"ts,json,graphql\" --exec \"ts-node user/index.ts\"",
    "invitation-dev": "nodemon --watch \"invitation/**\" --ext \"ts,json,graphql\" --exec \"ts-node invitation/index.ts\"",
    "gateway-dev": "nodemon --watch \"gateway/**\" --ext \"ts,json,graphql\" --exec \"ts-node gateway/index.ts\"",
    "network-dev": "nodemon --watch \"network/**\" --ext \"ts,json,graphql\" --exec \"ts-node network/index.ts\"",
    "member-dev": "nodemon --watch \"member/**\" --ext \"ts,json,graphql\" --exec \"ts-node member/index.ts\"",
    "node-dev": "nodemon --watch \"node/**\" --ext \"ts,json,graphql\" --exec \"ts-node node/index.ts\"",
    "subscriber-dev": "nodemon --watch \"subscriber/**\" --ext \"ts,json,graphql\" --exec \"ts-node subscriber/index.ts\"",
    "sim-dev": "nodemon --watch \"sim/**\" --ext \"ts,json,graphql\" --exec \"ts-node sim/index.ts\"",
    "rate-dev": "nodemon --watch \"rate/**\" --ext \"ts,json,graphql\" --exec \"ts-node rate/index.ts\"",
    "package-dev": "nodemon --watch \"package/**\" --ext \"ts,json,graphql\" --exec \"ts-node package/index.ts\"",
    "all-dev": "concurrently \"yarn org-dev\" \"yarn user-dev\" \"yarn network-dev\" \"yarn node-dev\" \"yarn subscriber-dev\" \"yarn sim-dev\" \"yarn package-dev\" \"yarn rate-dev\" \"yarn gateway-dev\" \"yarn invitation-dev\" \"yarn member-dev\" \"yarn planning-tool-dev\"",
    "planning-tool-dev": "prisma generate --schema=./planning-tool/prisma/schema.prisma && nodemon --watch \"planning-tool/**\" --ext \"ts,json,graphql\" --exec \"ts-node planning-tool/index.ts\"",
    "metrics-dev": "yarn clear-cache && nodemon --watch \"metrics/**\" --ext \"ts,json,graphql\" --exec \"ts-node metrics/index.ts\"",
    "all-start": "node dist/org/index.js & node dist/user/index.js & node dist/network/index.js & node dist/node/index.js & node dist/subscriber/index.js & node dist/sim/index.js & node dist/package/index.js & node dist/rate/index.js & node dist/invitation/index.js & node dist/member/index.js & node dist/planning-tool/index.js & node dist/gateway/index.js"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^6.2.2",
    "@apollo/gateway": "^2.7.4",
    "@apollo/server": "^4.10.4",
    "@apollo/subgraph": "^2.7.4",
    "@graphql-tools/schema": "^10.0.3",
    "@kvs/node-localstorage": "^2.2.0",
    "@prisma/client": "^5.12.1",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "graphql": "16.8.1",
    "graphql-scalars": "1.23.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "https": "^1.0.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "stripe": "^12.16.0",
    "tsc": "^2.0.4",
    "type-graphql": "2.0.0-beta.2",
    "typedi": "^0.10.0",
    "winston": "^3.13.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@kvs/types": "^2.1.4",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.5.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.6.2",
    "prisma": "^5.4.2",
    "string-width": "^6.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "jackspeak": "^2.3.6"
  }
}
