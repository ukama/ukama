    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.3"
  }
